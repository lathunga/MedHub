<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.medhub">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".register" />
        <activity android:name=".patient_home" />
        <activity android:name=".doctor_home" />
        <activity android:name=".admin_home" />
        <activity android:name=".contact_list" />
        <activity android:name=".add_contact" />
        <activity android:name=".instant_message" />
        <activity android:name=".appointments" />
        <activity android:name=".make_appointment" />
        <activity android:name=".view_appointments" />
    </application>

</manifest>